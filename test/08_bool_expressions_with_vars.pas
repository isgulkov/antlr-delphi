
var
a, b : boolean;
begin
	

	a := True;
	writeln('a equals...');
	writeln(a);

	b := False;
	writeln('b equals...');
	writeln(b);

	writeln('a and b equals...');
	writeln(a and b);

	writeln('a or b equals...');
	writeln(a or b);

	writeln('not a equals...');
	writeln(not a);

	writeln('a and a equals...');
	writeln(a and a);

end.

